<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.VBox?>
<TitledPane xmlns:fx="http://javafx.com/fxml/1" fx:id="jvmsTitlePane" animated="false" collapsible="false"
            maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Running JVMs"
            xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="com.github.naton1.jvmexplorer.fx.jvms.RunningJvmsController">
    <content>
        <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
                <TextField fx:id="searchJvms" promptText="Search JVMs"/>
                <ListView fx:id="processes" prefHeight="200.0" prefWidth="200.0" styleClass="dense" VBox.vgrow="ALWAYS"/>
            </children>
        </VBox>
    </content>
</TitledPane>
