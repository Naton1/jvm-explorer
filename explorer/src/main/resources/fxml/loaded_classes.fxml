<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<TitledPane xmlns:fx="http://javafx.com/fxml/1" fx:id="classesTitlePane" animated="false" collapsible="false"
            maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Loaded Classes"
            xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="com.github.naton1.jvmexplorer.fx.classes.LoadedClassesController">
    <content>
        <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
                <HBox alignment="CENTER_LEFT">
                    <children>
                        <TextField fx:id="searchClasses" promptText="Search classes" HBox.hgrow="ALWAYS"/>
                    </children>
                </HBox>
                <TreeView fx:id="classes" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                          VBox.vgrow="ALWAYS"/>
            </children>
        </VBox>
    </content>
</TitledPane>
